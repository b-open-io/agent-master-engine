{
  "servers": {
    "github-env": {
      "transport": "stdio",
      "command": "github-mcp",
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "github-input": {
      "transport": "stdio", 
      "command": "github-mcp",
      "env": {
        "GITHUB_TOKEN": "${input:github_token}"
      }
    }
  }
}